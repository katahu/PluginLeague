{
  "manifest_version": 3,
  "name": "Tester",
  "version": "0.1",
  "description": "",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://game.league17.ru/*"],
      "js": [
        "script.js",
        "utils.js",
        "checker.js",
        "API.js",
        "UI.js",
        "capture.js",
        "heal.js",
        "development.js",
        "data.js",
        "newBot.js"
      ],
      "css": ["style/fonts.css", "style/style.css"],
      "type": "module"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js", "style/fa-light-300.woff2", "style/Roboto-Medium.woff2"],
      "matches": ["https://game.league17.ru/*"]
    }
  ]
}
